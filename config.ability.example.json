{
  "//": "This is an example configuration that we use on ##ability@freenode",
  "nick": "bobot",
  "channels": [
    "##ability",
    "#ed-209-arena"
  ],
  "server": "irc.freenode.net",
  "ssl": false,
  "port": 6667,
  "sasl": false,
  "flood": {
    "protection": true,
    "delay": 500
  },
  "plugins": [
    "memory",
    "caniuse",
    "alert",
    "conj",
    "fakeimg",
    ["forkyou", {
      "repoURL": "https://github.com/kud/ed-209"
    }],
    "list",
    ["reactions", {
      "maxDelay": 5,
      "patterns": {
        "metal": "\\m/",
        "hardcore": ["\\m/", "HxC"],
        "babtou fragile": "http://img.scoop.it/IODNGK0ewLu0Js9UlIy0yzl72eJkfbmt4t8yenImKBVvK0kTmF0xjctABnaLJIm9",
        "nul": ["nul nul nul", "oui, c'est nul :("],
        "(?:lol|mdr|ahah|haha|rofl)": [":)", ":D", ":P"],
        "(?:s(?:or)?ry|d[eé]sol[eé]|dsl|o[ou]ps|my bad)": "¯\\_(ツ)_/¯",
        "(?:th(anks|x)|merci)\\s+{@}": "de rine"
      }
    }],
    "logger",
    "stdin_reader"
  ]
}
